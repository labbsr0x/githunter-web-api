{
  "server": {
    "url": "localhost",
    "port": 5000,
    "baseDir": "/"
  },
  "githunterBindStarws": {
    "host": "http://localhost",
    "port": 3005,
    "endpoints": {
      "metrics": "/metrics"
    },
    "nodes": {
      "repositoryStats": "repositoryStats",
      "userStats": "userStats"
    },
    "providers": [
      "github",
      "gitlab"
    ],
    "limitDefault": "100",
    "quantityDaysDefault": 365
  },
  "githunterDataProvider": {
    "host": "http://localhost",
    "port": 3333,
    "endpoints": {
      "languages": "/languages"
    }
  },
  "githunterUserScore": {
    "host": "http://localhost",
    "port": 3009,
    "endpoints": {
      "score": "/score",
      "scoreUser": "/score/user"
    },
    "limitDefault": "100"
  }
}
